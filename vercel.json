{
  "headers": [
    {
      "source": "/public/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/public/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/plain"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
